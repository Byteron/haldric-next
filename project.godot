; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

[application]

config/name="Haldric Next"
run/main_scene="res://src/Main.tscn"
config/icon="res://icon.png"

[autoload]

Scenes="*res://src/nodes/global/Scenes.tscn"
Data="*res://src/nodes/global/Data.tscn"
Sfx="*res://src/nodes/global/Sfx.tscn"

[debug]

settings/fps/force_fps=60

[display]

window/size/width=1600
window/size/height=800
window/size/resizable=false
window/dpi/allow_hidpi=true
window/vsync/use_vsync=true

[editor]

export/convert_text_resources_to_binary=true

[importer_defaults]

scene={
"_subresources": {
},
"animation/fps": 15,
"animation/import": true,
"import_script/path": "",
"meshes/create_shadow_meshes": true,
"meshes/ensure_tangents": true,
"meshes/generate_lods": false,
"meshes/light_baking": 2,
"meshes/lightmap_texel_size": 0.1,
"nodes/root_name": "Scene Root",
"nodes/root_scale": 1.0,
"nodes/root_type": "Node3D",
"skins/use_named_skins": true
}

